          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "route": "FROM /messages/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "VisionSampleModule": {
      "properties.desired": {
        "ModelUrl": "",
        "LabelUrl": "",
        "ConfigUrl": "",
        "FreqToSendMsg": 12,
        "ObjectOfInterest": "ALL"
      }
    }
  }
